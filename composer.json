{
  "name": "ec-europa/phpdrush",
  "description": "Php library for interacting with Drush Drupal cli tool",
  "type": "library",
  "keywords": ["drush","drupal","helper","php"],
  "license": "EUPL",
  "homepage": "https://github.com/ec-europa/phpdrush",
  "authors": [
    {
      "name": "Gregory Boddin",
      "email": "gregory.boddin@ext.ec.europa.eu",
      "homepage": "https://github.com/gboddin"
    }
  ],
  "autoload": {
    "psr-4": {"PhpDrush\\": "src/"}
  },
  "require": {
    "php" : ">=5.3.0",
    "cheprasov/php-redis-client" : "~1.7.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~3.0.1",
	  "phpunit/phpunit": "5.3.*",
    "drush/drush": "8.1.10"
  },
  "config": {
    "bin-dir": "bin/"
  }
}
